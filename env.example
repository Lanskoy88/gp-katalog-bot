# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Moysklad API Configuration
MOYSKLAD_API_TOKEN=your_moysklad_api_token_here
MOYSKLAD_ACCOUNT_ID=your_moysklad_account_id_here

# Server Configuration
PORT=3000
NODE_ENV=production
BASE_URL=https://your-app-name.onrender.com

# Admin Configuration
ADMIN_IDS=123456789,987654321

# Security
JWT_SECRET=your_jwt_secret_here

# CORS Configuration (опционально)
ALLOWED_ORIGINS=https://web.telegram.org

# Railway Configuration (автоматически устанавливается Railway)
RAILWAY_STATIC_URL=https://your-app-name.railway.app

# Additional Configuration
USE_WEBHOOK=true 