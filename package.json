{
  "name": "gp-katalog-bot",
  "version": "1.0.0",
  "description": "Telegram mini-app для каталога товаров с интеграцией MoySklad",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "check-config": "node check-config.js",
    "check-render": "node check-render.js",
    "test-telegram": "node test-telegram.js",
    "setup-tunnel": "node setup-tunnel.js",
    "postinstall": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "node-telegram-bot-api": "^0.64.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "telegram",
    "bot",
    "moysklad",
    "catalog",
    "mini-app"
  ],
  "author": "Lanskoy88",
  "license": "MIT"
}
