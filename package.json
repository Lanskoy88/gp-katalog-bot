{
  "name": "gp-katalog-bot",
  "version": "1.0.0",
  "description": "Telegram mini-app для каталога товаров с интеграцией MoySklad",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "nodemon server/index.js",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "check-config": "node check-config.js",
    "check-render": "node check-render.js",
    "test-telegram": "node test-telegram.js",
    "setup-tunnel": "node setup-tunnel.js",
    "setup-webhook": "node setup-webhook.js",
    "postinstall": "cd client && npm install && npm run build"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "multer": "^1.4.5-lts.1",
    "node-telegram-bot-api": "^0.64.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "tough-cookie": "^4.1.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  },
  "keywords": [
    "telegram",
    "bot",
    "moysklad",
    "catalog",
    "mini-app"
  ],
  "author": "Lanskoy88",
  "license": "MIT"
}
